



MYSQL

 DROP TABLE EMP_RECORD;


CREATE  TABLE EMP_RECORD (  
empid INT NOT NULL ,  
firstname VARCHAR(45) NULL ,  
lastname VARCHAR(45) NULL ,  
email VARCHAR(45) NULL ,  
PRIMARY KEY (empid) );





WITH OUT PRIMARY KEY IN MYSQL

CREATE  TABLE EMP_RECORD (  
empid INT NOT NULL ,  
firstname VARCHAR(45) NULL ,  
lastname VARCHAR(45) NULL ,  
email VARCHAR(45) NULL);








 Oracle tABLE 

CREATE TABLE EMP_RECORD(
  empid number(6) NOT NULL,
  firstname VARCHAR(45) NULL ,  
  lastname VARCHAR(45) NULL ,  
  email VARCHAR(45) NULL );








